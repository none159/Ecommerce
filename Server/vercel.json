{
  "version": 2,
  "builds": [
    {
      "src": "/api/index.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "/data/dataimport.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
     "dest": "/api/index.cjs",
     "methods": ["GET", "POST", "PUT", "DELETE"],
     "headers": {
      "Access-Control-Allow-Credentials": "true",
       "Access-Control-Allow-Origin": "*",
       "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
       "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Authorisation, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
     }
    },
       {
      "src": "/api/users/login",
      "dest":"/data/dataimport.cjs",
     "methods": ["GET", "POST", "PUT", "DELETE"],
     "headers": {
      "Access-Control-Allow-Credentials": "true",
       "Access-Control-Allow-Origin": "*",
       "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
       "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Authorisation, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
     }
    }
    ]
  
}
